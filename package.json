{
  "name": "solana-copy-trader-service",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit",
    "lint": "echo \"Add ESLint configuration for linting\"",
    "webhook:example": "ts-node examples/webhook-example.ts",
    "webhook:simple": "ts-node examples/webhook-example.ts simple",
    "webhook:integrated": "ts-node examples/webhook-example.ts integrated",
    "webhook:production": "ts-node examples/webhook-example.ts production",
    "test:webhook": "curl -X POST http://localhost:3001/helius-webhook -H \"Content-Type: application/json\" -d '{\"test\": \"data\"}'",
    "health:check": "curl http://localhost:3001/health"
  },
  "keywords": [
    "typescript",
    "node",
    "backend",
    "solana",
    "copy-trading",
    "rabbitmq",
    "redis",
    "websocket",
    "webhook",
    "helius"
  ],
  "author": "",
  "license": "MIT",
  "description": "Real-time Solana copy trading service with blockchain monitoring and trade replication",
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/express": "^4.17.21",
    "@types/node": "^24.0.15",
    "@types/ws": "^8.5.10",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.95.2",
    "@types/uuid": "^9.0.7",
    "amqplib": "^0.10.3",
    "axios": "^1.6.0",
    "bs58": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "helius-sdk": "^1.3.6",
    "ioredis": "^5.3.2",
    "mongodb": "^6.18.0",
    "socket.io": "^4.7.5",
    "uuid": "^9.0.1",
    "ws": "^8.16.0"
  },
  "types": "dist/index.d.ts"
}
